<navigation:Page x:Class="SilverlightSampleWithCaliburnMicro.Views.MainWindowView" 
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           mc:Ignorable="d"
           xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
        Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="171*"/>
            <ColumnDefinition Width="168*"/>
            <ColumnDefinition Width="178*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="34*"/>
            <RowDefinition Height="15*"/>
            <RowDefinition Height="271*"/>
        </Grid.RowDefinitions>
        <Button Name="HelloWorld" Grid.Row="0" Grid.Column="0" Margin="2">Hello World View Model</Button>
        <Button Name="SeeSample" Grid.Row="0" Grid.Column="1" Margin="2">Sample View Model</Button>
        <Button Name="SeeAnotherSample" Grid.Row="0" Grid.Column="2" Margin="2">Send Messages</Button>
        <ContentControl Name="ActiveItem" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3" Margin="2"/>
    </Grid>
</navigation:Page>
